        <fields>
            <name>Help_Text</name>
            <fieldText>You are creating a new Support Case and Inbound Call for {!AccountName} at {!varPhone}.

Please enter the case and call information below:</fieldText>
            <fieldType>DisplayText</fieldType>
            <isVisible>true</isVisible>
        </fields>
        <fields>
            <name>Case_Subject</name>
            <dataType>String</dataType>
            <fieldText>Case Subject</fieldText>
            <fieldType>InputField</fieldType>
            <helpText>This Subject may be sent to the customer as part of a Post-Case Survey.</helpText>
            <isRequired>true</isRequired>
            <isVisible>true</isVisible>
        </fields>
        <fields>
            <name>Case_Status</name>
