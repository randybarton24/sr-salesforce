            <dataType>String</dataType>
            <fieldText>Office Contact</fieldText>
            <fieldType>RadioButtons</fieldType>
            <isRequired>true</isRequired>
            <isVisible>true</isVisible>
        </fields>
        <fields>
            <name>Case_Description</name>
            <fieldText>Case/Call Description</fieldText>
            <fieldType>LargeTextArea</fieldType>
            <isRequired>false</isRequired>
            <isVisible>true</isVisible>
            <validationRule>
                <errorMessage>The value you entered for Case Descriptions is too long.  Please reduce the length under 255 characters</errorMessage>
                <formulaExpression>LEN({!Case_Description})&lt;550</formulaExpression>
            </validationRule>
        </fields>
        <fields>
            <name>Case_Resolution</name>
            <fieldText>Case Resolution</fieldText>
            <fieldType>LargeTextArea</fieldType>
            <isRequired>false</isRequired>
            <isVisible>true</isVisible>
            <validationRule>
                <errorMessage>The value you entered for Case Resolution is too long.  Please reduce it to under 255 characters.</errorMessage>
                <formulaExpression>LEN({!Case_Resolution})&lt;550</formulaExpression>
            </validationRule>
        </fields>
        <fields>
            <name>Call_Subject</name>
            <dataType>String</dataType>
            <fieldText>Call Subject</fieldText>
            <fieldType>InputField</fieldType>
            <isRequired>true</isRequired>
            <isVisible>true</isVisible>
