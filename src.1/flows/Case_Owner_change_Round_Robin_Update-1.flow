    <description>Updates the Round Robin when the owner changes and Updates the account when a sync list case is opened or closed</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>And( ISCHANGED( [Case].OwnerId ), OR([Case].RecordTypeId = &quot;012i0000001EAmdAAG&quot; , [Case].RecordTypeId = &quot;012i0000001EALwAAO&quot;, [Case].RecordTypeId=&quot;012i0000001DnilAAC&quot;))</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>And( ISCHANGED( {!myVariable_current.OwnerId} ), OR({!myVariable_current.RecordTypeId} = &quot;012i0000001EAmdAAG&quot; , {!myVariable_current.RecordTypeId} = &quot;012i0000001EALwAAO&quot;, {!myVariable_current.RecordTypeId}=&quot;012i0000001DnilAAC&quot;))</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
