            <value>
                <stringValue>And( ISCHANGED( [Case].OwnerId ), OR([Case].RecordTypeId = &quot;012i0000001EAmdAAG&quot; , [Case].RecordTypeId = &quot;012i0000001EALwAAO&quot;, [Case].RecordTypeId=&quot;012i0000001DnilAAC&quot;))</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1_pmetrule</name>
        <dataType>Boolean</dataType>
        <expression>And( ISCHANGED( {!myVariable_old.OwnerId} ), OR({!myVariable_old.RecordTypeId} = &quot;012i0000001EAmdAAG&quot; , {!myVariable_old.RecordTypeId} = &quot;012i0000001EALwAAO&quot;, {!myVariable_old.RecordTypeId}=&quot;012i0000001DnilAAC&quot;))</expression>
    </formulas>
