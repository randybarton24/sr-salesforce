<apex:page renderAs="pdf" controller="ContractPDF_Controller" sidebar="false" showheader="false" standardStylesheets="false" cache="false" applyHtmlTag="false" >
    
    <html lang="en">
        <head>
            <meta charset="utf-8"/>
            <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
            <meta name="viewport" content="width=device-width, initial-scale=1"/>
            <meta name="description" content=""/>
            <meta name="author" content=""/>
            
            
            <title>Solutionreach Agreement</title>
            
            <!-- Bootstrap core CSS -->
            <apex:stylesheet value="{!URLFOR($Resource.bootstrapPDF, 'css/bootstrap.min.css')}" />
            <apex:stylesheet value="{!URLFOR($Resource.bootstrap, 'css/bootstrap-theme.min.css')}" />
            <apex:stylesheet value="{!URLFOR($Resource.style)}" />
            
            <style type="text/css">
                
                @page {
                size: A4;
                }
                
                * {
                font-size: 10px;
                }
                
                sup {
                font-size: 6px;
                }
                
                ol {
                margin-right: 10px;
                }
                
                .table > tbody > tr > td {
                height: 10px;
                padding: 1px;
                margin: 1px;
                padding-left: 3px;
                vertical-align: middle;
                white-space: nowrap;
                }
                
                .table > tbody > tr > td > p {
                padding: 1px;
                margin: 1px;
                padding-left: 3px;
                vertical-align: middle;
                white-space: nowrap;
                }
                
                .table > thead > tr > td {
                height: 10px;
                padding: 1px;
                margin: 1px;
                padding-left: 3px;
                vertical-align: middle;
                white-space: nowrap;
                }
                
                .table > thead > tr > td > p {
                padding: 1px;
                margin: 1px;
                padding-left: 3px;
                vertical-align: middle;
                white-space: nowrap;
                }
                
                .nobreak-div {
                page-break-inside: avoid;
                }
                
                .EULA-BOLD {
                
                font-size:10px;
                font-weight:bold;
                padding: 0px;
                margin: 0px;
                border: 0px;
                line-height: normal;
                }
                
                .EULA-NORMAL {
                
                font-size:10px;
                font-weight:normal;
                padding: 0px;
                margin: 0px;
                border: 0px;
                line-height: normal;
                }
                
                .nobr {
                white-space: nowrap;
                }
                
            </style>
            
            
        </head>
        
        <body >
            
            <div class="container theme-showcase" role="main">
                <apex:form >
                    <apex:outputPanel id="Header">
                        <div id="Heading" style="padding: 1px; margin: 1px;" class="page-header">
                            <header style="height: 92px; border-bottom: none; background: #FFFFFF url('{!$Resource.bgSRLogo}') no-repeat center center;"/>
                            <apex:outputPanel id="HeaderTitle" rendered="{!!isAPI}">
                                <div>
                                    <p class="text-uppercase text-center"><strong>Solutionreach Services Agreement</strong></p>
                                </div>
                            </apex:outputPanel>
                        </div>
                        <apex:outputPanel id="Preamble" rendered="{!!isAPI}">
                            <div id="Preamble">
                                <p>This Services Agreement (“Agreement”) is entered into on <apex:outputText value="{0,date, MMMM d',' yyyy}"><apex:param Value="{!TODAY()}"/></apex:outputText> ("Effective Date") by and between Solutionreach, Inc. (the “Company”), with its principal place of business at 2600 N. Ashton Blvd. Lehi, UT 84043, and Client, with its principal place of business listed below. This Agreement governs the rights and responsibilities of the parties with respect to the Services identified below, and in consideration of the mutual promises contained herein, the parties agree to be legally bound by its terms.</p>
                            </div>
                        </apex:outputPanel>
                    </apex:outputPanel>
                    <apex:outputPanel id="InformationTable" rendered="{!!isAPI}">
                        <div id="Information Table">
                            <div class="row">
                                <div>
                                    <table class="table table-bordered" style="padding: 1px;">
                                        <thead>
                                            <tr>
                                                <td colspan="6" class="bg-black">
                                                    <p><strong>Client Account Information ("Client")</strong></p>
                                                </td>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td colspan="1" class="bg-gray">
                                                    <p><strong>Legal Business Name</strong></p>
                                                </td>    
                                                <td colspan="5">
                                                    <apex:outputText value="{!acctWrapper.BusinessName}" />
                                                </td>
                                            </tr>
                                            <tr>
                                                <td colspan="1" class="bg-gray">
                                                    <p><strong>Business Address</strong></p>
                                                </td>
                                                <td colspan="5">
                                                    <apex:outputText value="{!acctWrapper.Address1}" />
                                                </td>
                                            </tr>
                                            <tr>
                                                <td colspan="1" class="bg-gray">
                                                    <p><strong>City</strong></p>
                                                </td>
                                                <td colspan="1">
                                                    <apex:outputText value="{!acctWrapper.City}" />
                                                </td>
                                                <td colspan="1" class="bg-gray">
                                                    <p><strong>State</strong></p>
                                                </td>
                                                <td colspan="1">
                                                    <apex:outputText value="{!acctWrapper.State}" />
                                                </td>
                                                <td colspan="1" class="bg-gray">
                                                    <p><strong>Zip</strong></p>
                                                </td>
                                                <td colspan="1" valign="center">
                                                    <apex:outputText value="{!acctWrapper.Zip}" />
                                                </td>
                                            </tr>
                                            <tr>
                                                <td colspan="1" class="bg-gray">
                                                    <p><strong>Country</strong></p>
                                                </td>
                                                <td colspan="5">
                                                    <apex:outputText value="{!acctWrapper.Country}" />
                                                </td>
                                            </tr>
                                            <tr>
                                                <td colspan="1" class="bg-gray">
                                                    <p><strong>Website Address</strong></p>
                                                </td>
                                                <td colspan="5">
                                                    <apex:outputText value="{!acctWrapper.Website}" />
                                                </td>
                                            </tr>
                                            <tr>
                                                <td colspan="1" class="bg-gray">
                                                    <p><strong>Practice Software</strong></p>
                                                </td>
                                                <td colspan="2">
                                                    <apex:outputText value="{!opp.Account.Practice_Management_Software__r.Name}"/>
                                                </td>
                                                <td colspan="1" class="bg-gray">
                                                    <p><strong>Version</strong></p>
                                                </td>
                                                <td colspan="2">
                                                    <apex:outputText value="{!opp.Account.Practice_Management_Software__r.Version__c}"/>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <br/>
                    </apex:outputPanel>
                    <apex:outputPanel id="ContactTable" rendered="{!!isAPI}">
                        
                        <apex:outputText value="TEST - NON PAYWALL: This Contact Grid is displayed for all sales." rendered="{!isTest}"/>
                        <div id="Contact Table">
                            <div class="row text-center">
                                <div>
                                    <apex:outputPanel rendered="{!hasSR || hasSRMonthly || hasSRAdvance || hasSRAnnual || isTest}">
                                        <table class="table table-bordered">
                                            <thead>
                                                <tr>
                                                    <td colspan="7" class="bg-black" style="text-align: left;">
                                                        <p><strong>Office Contact Information</strong></p>
                                                    </td>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <td colspan="1" class="bg-gray">
                                                        <p><strong>Office Role</strong></p>
                                                    </td>
                                                    <td colspan="1" class="bg-gray">
                                                        <p><strong>First Name</strong></p>
                                                    </td>
                                                    <td colspan="1" class="bg-gray">
                                                        <p><strong>Last Name</strong></p>
                                                    </td>
                                                    <td colspan="1" class="bg-gray">
                                                        <p><strong>Title</strong></p>
                                                    </td>
                                                    <td colspan="1" class="bg-gray">
                                                        <p><strong>Phone</strong></p>
                                                    </td>
                                                    <td colspan="1" class="bg-gray">
                                                        <p><strong>Email</strong></p>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td colspan="1" height="30">
                                                        Primary Doctor/Owner
                                                    </td>
                                                    <td colspan="1">
                                                        <apex:outputText value="{!primaryDoctorWrapper.FirstName}"/>
                                                    </td>
                                                    <td colspan="1">
                                                        <apex:outputText value="{!primaryDoctorWrapper.LastName}"/>
                                                    </td>
                                                    <td colspan="1">
                                                        <apex:outputText value="{!primaryDoctorWrapper.Title}"/>
                                                    </td>
                                                    <td colspan="1">
                                                        <apex:outputText value="{!primaryDoctorWrapper.DirectPhone}"/>
                                                    </td>
                                                    <td colspan="1">
                                                        <apex:outputText value="{!primaryDoctorWrapper.Email}"/>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td colspan="1" height="30">
                                                        Business Contact
                                                    </td>
                                                    <td colspan="1">
                                                        <apex:outputText value="{!businessContactWrapper.FirstName}"/>
                                                    </td>
                                                    <td colspan="1">
                                                        <apex:outputText value="{!businessContactWrapper.LastName}"/>
                                                    </td>
                                                    <td colspan="1">
                                                        <apex:outputText value="{!businessContactWrapper.Title}"/>
                                                    </td>
                                                    <td colspan="1">
                                                        <apex:outputText value="{!businessContactWrapper.DirectPhone}"/>
                                                    </td>
                                                    <td colspan="1">
                                                        <apex:outputText value="{!businessContactWrapper.Email}"/>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </apex:outputPanel>
                                </div>
                            </div>
                        </div>
                    </apex:outputPanel>
                    <apex:outputPanel id="BillingContact" rendered="{!!isAPI}">
                        <apex:outputText value="TEST - NON PAYWALL: This Billing Contact Section is displayed for all sales." rendered="{!isTest}"/>
                        <div id="Billing Contact">
                            <div class="row">
                                <div>
                                    <apex:outputPanel rendered="{!hasSR || hasSRMonthly || hasSRAdvance || hasSRAnnual || isTest}">
                                        <table class="table table-bordered">
                                            <thead>
                                                <tr>
                                                    <td colspan="6" class="bg-black" style="text-align: left;">
                                                        <p><strong>Billing Contact Information</strong></p>
                                                    </td>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <td colspan="1" class="bg-gray">
                                                        <p><strong>First Name</strong></p>
                                                    </td>
                                                    <td colspan="1">
                                                        <apex:outputText value="{!billingContactWrapper.FirstName}"/>
                                                    </td>
                                                    <td colspan="1" class="bg-gray">
                                                        <p><strong>Last Name</strong></p>
                                                    </td>
                                                    <td colspan="1">
                                                        <apex:outputText value="{!billingContactWrapper.LastName}"/>
                                                    </td>
                                                    <td colspan="1" class="bg-gray">
                                                        <p><strong>Title</strong></p>
                                                    </td>
                                                    <td colspan="1">
                                                        <apex:outputText value="{!billingContactWrapper.Title}"/>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td colspan="1" class="bg-gray">
                                                        <p><strong>Phone</strong></p>
                                                    </td>
                                                    <td colspan="1">
                                                        <apex:outputText value="{!billingContactWrapper.DirectPhone}"/>
                                                    </td>
                                                    <td colspan="1" class="bg-gray">
                                                        <p><strong>Email</strong></p>
                                                    </td>
                                                    <td colspan="3">
                                                        <apex:outputText value="{!billingContactWrapper.Email}"/>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </apex:outputPanel>
                                </div>
                            </div>
                        </div>
                    </apex:outputPanel>
                    <br/>
                    <apex:outputPanel id="ProductsGrid" rendered="{!!isAPI}">
                        <div id="Products Main">
                            <apex:outputPanel id="NoDiscount" rendered="{!!hasDiscount || isTest}">
                                <apex:outputText value="TEST - NON PAYWALL: This Product Grid is displayed for all sales without a discount." rendered="{!isTest}"/>
                                <div id="Product Grid">
                                    <div class="row text-center">
                                        <div>
                                            <table class="table table-bordered">
                                                <thead>
                                                    <tr>
                                                        <td colspan="7" class="bg-black" style="text-align: left;">
                                                            <p><strong>Product Selections</strong></p>
                                                        </td>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr>
                                                        <td colspan="1" class="bg-gray">
                                                            <p><strong>Product Name</strong></p>
                                                        </td>
                                                        <td colspan="1" class="bg-gray">
                                                            <p><strong>Term</strong></p>
                                                        </td>
                                                        <td colspan="1" class="bg-gray">
                                                            <p><strong>Setup<br/>Fee</strong></p>
                                                        </td>
                                                        <td colspan="1" class="bg-gray">
                                                                    
                                                            <!-- Annual vs. Monthly -->
                                                            <apex:outputText rendered="{!!hasSRAnnual}">
                                                                <p><strong>Monthly<br/>Service Fee</strong></p>
                                                            </apex:outputText>
                                                            <apex:outputText rendered="{!hasSRAnnual}">
                                                                <p><strong>Annual<br/>Service Fee</strong></p>
                                                            </apex:outputText> 
                                                        </td>
                                                        <td colspan="1" class="bg-gray">
                                                            <p><strong>Additional<br/>Providers</strong></p>
                                                        </td>
                                                        <td colspan="1" class="bg-gray">
                                                                    
                                                            <!-- Annual vs. Monthly -->
                                                            <apex:outputText rendered="{!!hasSRAnnual}">
                                                                <p><strong>Total Monthly<br/>Service Fees</strong></p>
                                                            </apex:outputText>
                                                            <apex:outputText rendered="{!hasSRAnnual}">
                                                                <p><strong>Total Annual<br/>Service Fees</strong></p>
                                                            </apex:outputText>  
                                                        </td>
                                                    </tr>
                                                    <apex:repeat value="{!wrapProductList}" var="product">
                                                        <tr>
                                                            <td colspan="1" height="30">
                                                                <apex:outputText value="{!product.productName}"/>
                                                            </td>
                                                            <td colspan="1">
                                                                <apex:outputText value="{!product.term}"/>
                                                            </td>
                                                            <td colspan="1">
                                                                <apex:outputText value="{0, Number, Currency}">
                                                                    <apex:param value="{!product.setupFee}"/>
                                                                </apex:outputText>
                                                            </td>
                                                            <td colspan="1">
                                                                <apex:outputText value="{0, Number, Currency}">
                                                                    <apex:param value="{!product.monthlyFee}"/>
                                                                </apex:outputText>
                                                                &nbsp;
                                                                <apex:outputText value="{!product.promoPriceText}"/>
                                                            </td>
                                                            <td colspan="1">
                                                                <apex:outputText value="N/A" rendered="{!product.addtlProviderQty = 0}"/>
                                                                <apex:outputText value="# Addtl: {0, Number, Integer}" rendered="{!product.addtlProviderQty != 0}">
                                                                    <apex:param value="{!product.addtlProviderQty}"/>
                                                                </apex:outputText>
                                                                <br/>
                                                                <apex:outputText value="x {0, Number, Currency} = {1, Number, Currency}" rendered="{!product.addtlProviderQty != 0}">
                                                                    <apex:param value="{!product.addtlProviderPrice}"/>
                                                                    <apex:param value="{!product.addtlProviderPrice * product.addtlProviderQty}"/>
                                                                </apex:outputText>
                                                            </td>
                                                            <td colspan="1">
                                                                        
                                                                <!-- Annual vs. Monthly -->
                                                                <apex:outputText value="{0, Number, Currency}/mo"  rendered="{!product.billingTerm != 'AdvanceAnnual'}">
                                                                    <apex:param value="{!product.totalMonthlyFee}"/>
                                                                </apex:outputText>
                                                                <apex:outputText value="{0, Number, Currency}/yr"  rendered="{!product.billingTerm = 'AdvanceAnnual'}">
                                                                    <apex:param value="{!product.totalMonthlyFee}"/>
                                                                </apex:outputText>
                                                            </td>
                                                        </tr>
                                                    </apex:repeat>
                                                    <tr>
                                                        <td class="text-left" colspan="5">
                                                                    
                                                            <!-- Annual vs. Monthly -->
                                                            <apex:outputText rendered="{!!hasSRAnnual}">
                                                                <p><strong>Grand Total Monthly Service Fees:</strong></p>
                                                            </apex:outputText>
                                                            <apex:outputText rendered="{!hasSRAnnual}">
                                                                <p><strong>Grand Total Annual Service Fees:</strong></p>
                                                            </apex:outputText>
                                                        </td>
                                                        <td colspan="1">
                                                            
                                                            <!-- Annual vs. Monthly -->
                                                            <apex:outputText value="{0, Number, Currency} per month" rendered="{!!hasSRAnnual}">
                                                                <apex:param value="{!grandTotalMonthlyFee}"/>
                                                            </apex:outputText>
                                                            <apex:outputText value="{0, Number, Currency} per year" rendered="{!hasSRAnnual}">
                                                                <apex:param value="{!grandTotalMonthlyFee}"/>
                                                            </apex:outputText>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </apex:outputPanel>
                            <apex:outputPanel id="Discount" rendered="{!hasDiscount || isTest}">
                                <apex:outputText value="TEST - NON PAYWALL: This Product Grid is displayed for all sales with a discount." rendered="{!isTest}"/>
                                <div id="Product Grid">
                                    <div class="row text-center">
                                        <div>
                                            <table class="table table-bordered">
                                                <thead>
                                                    <tr>
                                                        <td colspan="7" class="bg-black"  style="text-align: left;">
                                                            <p><strong>Product Selections</strong></p>
                                                        </td>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr>
                                                        <td colspan="1" class="bg-gray">
                                                            <p><strong>Product Name</strong></p>
                                                        </td>
                                                        <td colspan="1" class="bg-gray">
                                                            <p><strong>Term</strong></p>
                                                        </td>
                                                        <td colspan="1" class="bg-gray">
                                                            <p><strong>Setup<br/>Fee</strong></p>
                                                        </td>
                                                        <td colspan="1" class="bg-gray">
                                                                    
                                                            <!-- Annual vs. Monthly -->
                                                            <apex:outputText rendered="{!!hasSRAnnual}">
                                                                <p><strong>Monthly<br/>Service Fee</strong></p>
                                                            </apex:outputText>
                                                            <apex:outputText rendered="{!hasSRAnnual}">
                                                                <p><strong>Annual<br/>Service Fee</strong></p>
                                                            </apex:outputText> 
                                                        </td>
                                                        <td colspan="1" class="bg-gray">
                                                            <p><strong>Promotional<br/>Discount</strong></p>
                                                        </td>
                                                        <td colspan="1" class="bg-gray">
                                                            <p><strong>Additional<br/>Providers</strong></p>
                                                        </td>
                                                        <td colspan="1" class="bg-gray">
                                                                    
                                                            <!-- Annual vs. Monthly -->
                                                            <apex:outputText rendered="{!!hasSRAnnual}">
                                                                <p><strong>Total Monthly<br/>Service Fees</strong></p>
                                                            </apex:outputText>
                                                            <apex:outputText rendered="{!hasSRAnnual}">
                                                                <p><strong>Total Annual<br/>Service Fees</strong></p>
                                                            </apex:outputText>  
                                                        </td>
                                                    </tr>
                                                    <apex:repeat value="{!wrapProductList}" var="product">
                                                        <tr>
                                                            <td colspan="1" height="30">
                                                                <apex:outputText value="{!product.productName}"/>
                                                            </td>
                                                            <td colspan="1">
                                                                <apex:outputText value="{!product.term}"/>
                                                            </td>
                                                            <td colspan="1">
                                                                <apex:outputText value="{0, Number, Currency}">
                                                                    <apex:param value="{!product.setupFee}"/>
                                                                </apex:outputText>
                                                            </td>
                                                            <td colspan="1">
                                                                <apex:outputText value="{0, Number, Currency}">
                                                                    <apex:param value="{!product.monthlyFee}"/>
                                                                </apex:outputText>
                                                                &nbsp;
                                                                <apex:outputText value="{!product.promoPriceText}"/>
                                                            </td>
                                                            <td colspan="1">
                                                                <apex:outputText value="{0, Number, Currency}">
                                                                    <apex:param value="{! -product.discountAmount}"/>
                                                                </apex:outputText>
                                                            </td>
                                                            <td colspan="1">
                                                                <apex:outputText value="N/A" rendered="{!product.addtlProviderQty = 0}"/>
                                                                <apex:outputText value="# Addtl: {0, Number, Integer}" rendered="{!product.addtlProviderQty != 0}">
                                                                    <apex:param value="{!product.addtlProviderQty}"/>
                                                                </apex:outputText>
                                                                <br/>
                                                                <apex:outputText value="x {0, Number, Currency} = {1, Number, Currency}" rendered="{!product.addtlProviderQty != 0}">
                                                                    <apex:param value="{!product.addtlProviderPrice}"/>
                                                                    <apex:param value="{!product.addtlProviderPrice * product.addtlProviderQty}"/>
                                                                </apex:outputText>
                                                            </td>
                                                            <td colspan="1">
                                                                        
                                                                <!-- Annual vs. Monthly -->
                                                                <apex:outputText value="{0, Number, Currency}/mo"  rendered="{!product.billingTerm != 'AdvanceAnnual'}">
                                                                    <apex:param value="{!product.totalMonthlyFee}"/>
                                                                </apex:outputText>
                                                                <apex:outputText value="{0, Number, Currency}/yr"  rendered="{!product.billingTerm = 'AdvanceAnnual'}">
                                                                    <apex:param value="{!product.totalMonthlyFee}"/>
                                                                </apex:outputText>
                                                            </td>
                                                        </tr>
                                                    </apex:repeat>
                                                    <tr>
                                                        <td class="text-left" colspan="6">
                                                                    
                                                            <!-- Annual vs. Monthly -->
                                                            <apex:outputText rendered="{!!hasSRAnnual}">
                                                                <p><strong>Grand Total Monthly Service Fees:</strong></p>
                                                            </apex:outputText>
                                                            <apex:outputText rendered="{!hasSRAnnual}">
                                                                <p><strong>Grand Total Annual Service Fees:</strong></p>
                                                            </apex:outputText>
                                                        </td>
                                                        <td colspan="1">
                                                            
                                                            <!-- Annual vs. Monthly -->
                                                            <apex:outputText value="{0, Number, Currency} per month" rendered="{!!hasSRAnnual}">
                                                                <apex:param value="{!grandTotalMonthlyFee}"/>
                                                            </apex:outputText>
                                                            <apex:outputText value="{0, Number, Currency} per year" rendered="{!hasSRAnnual}">
                                                                <apex:param value="{!grandTotalMonthlyFee}"/>
                                                            </apex:outputText>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </apex:outputPanel>
                            <apex:outputPanel id="Services" rendered="{!hasPS && !hasDiscountPS}">    
                                        <div id="Service Grid">
                                            <div class="row text-center">
                                                <div>
                                                    <table class="table table-bordered">    <tbody>
                                                            <tr>
                                                                <td colspan="1" class="bg-gray">
                                                                    <p><strong>Professional Services</strong></p>
                                                                </td>
                                                                <td colspan="1" class="bg-gray">
                                                                    <p><strong>Term</strong></p>
                                                                </td>
                                                                <td colspan="1" class="bg-gray">
                                                                    <p><strong>Fee</strong></p>
                                                                </td>
                                                                
                                                                <td colspan="1" class="bg-gray">
                                                                    <p><strong>Quantity
                                                                    </strong></p>
                                                                </td>
                                                                <td colspan="1" class="bg-gray">
                                                                    
                                                                    <p><strong>Subtotal</strong></p>
                                                      
                                                                </td>
                                                            </tr>
                                                            <apex:repeat value="{!wrapServicesList}" var="product">
                                                                <tr>
                                                                    <td colspan="1" height="30">
                                                                        <apex:outputText value="{!product.productName}"/>
                                                                    </td>
