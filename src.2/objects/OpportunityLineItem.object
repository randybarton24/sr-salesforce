    <fields>
        <fullName>Approval_Details__c</fullName>
        <externalId>false</externalId>
        <formula>IF( ISPICKVAL( PricebookEntry.Product2.RecurSoft__Billing_Frequency__c , &quot;Monthly&quot; ) ,
PricebookEntry.Product2.Name &amp; &quot;: &lt;br&gt;&quot;  &amp;
			&quot;$&quot; &amp; TEXT(UnitPrice) &amp; &quot;(&quot; &amp; TEXT(ROUND(Price_Discount__c, 4) * 100) &amp; &quot;% Discount) &lt;br&gt;&quot;  &amp;
IF(Number_of_Free_Months__c &gt; 0 , TEXT(Number_of_Free_Months__c) &amp; &quot; Orientation Months at $&quot; &amp; TEXT(Promotional_Price__c) &amp; &quot; &lt;br&gt;&quot; , &quot;&quot; ) &amp;
IF(ISBLANK(TEXT(Trial__c)), &quot;0&quot; , TEXT(Trial__c)) &amp; &quot; Day Trial &lt;br&gt;&quot;  &amp;
			IF( PricebookEntry.Product2.Default_Contract_Term__c = 1 , &quot;1&quot; , TEXT(Opportunity.RecurSoft__Initial_Contract_Term__c)) &amp; &quot; Month Term&quot;,

PricebookEntry.Product2.Name &amp; &quot;: &lt;br&gt;&quot;  &amp;
			&quot;$&quot; &amp; TEXT(UnitPrice) &amp; 
&quot;(&quot; &amp; TEXT(ROUND(Price_Discount__c, 4) * 100) &amp; &quot;% Discount)&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Approval Details</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Approval_Level_Required_Free_Months__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Opportunity.Owner.Approval_Exempt__c = true , 0 ,
	IF( Number_of_Free_Months__c &gt; 
		MAX(Approved_Free_Months__c, PricebookEntry.Level_2_Limit_Free_Months__c ) , 3 , 
		IF( Number_of_Free_Months__c &gt; 
			MAX(Approved_Free_Months__c, PricebookEntry.Level_1_Limit_Free_Months__c ) , 2 , 
			IF( Number_of_Free_Months__c &gt; 
				MAX(Approved_Free_Months__c, PricebookEntry.Level_0_Limit_Free_Months__c ) , 1 , 0
			)
		)
	)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Approval Level Required - Free Months</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Approval_Level_Required_Price_Discount__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Opportunity.Owner.Approval_Exempt__c = true , 0 ,
	IF( List_Price_Override__c &gt; 0 ,
		IF( UnitPrice &gt;= /*If the price discount is less than level 1 threshold, 0 Approval Level*/ 
			(List_Price_Override__c - 
				(List_Price_Override__c * 
					MAX( Approved_Price_Discount__c,PricebookEntry.Level_0_Limit_Price_Discount__c )
				)
			) , 0 ,
			IF( UnitPrice &gt;= 
				(List_Price_Override__c - 
					(List_Price_Override__c * 
						MAX( Approved_Price_Discount__c, PricebookEntry.Level_1_Limit_Price_Discount__c )
					)
				) , 1 ,
				IF( UnitPrice &gt;= 
					(List_Price_Override__c - 
						(List_Price_Override__c * 
							MAX( Approved_Price_Discount__c, PricebookEntry.Level_2_Limit_Price_Discount__c )
						)
					) , 2 , 3
				)
			)
		)  ,
		IF( UnitPrice &gt;= 
			(PricebookEntry.UnitPrice - 
				(PricebookEntry.UnitPrice * 
					MAX( Approved_Price_Discount__c, PricebookEntry.Level_0_Limit_Price_Discount__c )
				)
			) , 0 ,
			IF( UnitPrice &gt;= 
				(PricebookEntry.UnitPrice - 
					(PricebookEntry.UnitPrice * 
						MAX( Approved_Price_Discount__c, PricebookEntry.Level_1_Limit_Price_Discount__c )
					)
				) , 1 ,
				IF( UnitPrice &gt;= 
					(PricebookEntry.UnitPrice - 
						(PricebookEntry.UnitPrice * 
							MAX( Approved_Price_Discount__c, PricebookEntry.Level_2_Limit_Price_Discount__c )
						)
					) , 2,  3
				)
			)
		)
	)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Approval Level Required - Price Discount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
