        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    ISBLANK(TEXT(RecurSoft__Invoicing_Date__c))
)</errorConditionFormula>
        <errorDisplayField>RecurSoft__Invoicing_Date__c</errorDisplayField>
        <errorMessage>Please select a value for Invoicing Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__MilestonesRevenueTransaction</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    AND(
        ISPICKVAL(RecurSoft__Billing_Frequency__c, &#39;Milestones&#39;),
        OR(
          ISPICKVAL(RecurSoft__Rev_Installments_Period__c, &#39;Milestones&#39;),
          ISPICKVAL(RecurSoft__Rev_Installments_Period__c, &#39;One-Time&#39;),
          ISPICKVAL(RecurSoft__Rev_Installments_Period__c, &#39;Monthly&#39;),
          ISPICKVAL(RecurSoft__Rev_Installments_Period__c, &#39;Quarterly&#39;),
          ISPICKVAL(RecurSoft__Rev_Installments_Period__c, &#39;Semi-annual&#39;),
          ISPICKVAL(RecurSoft__Rev_Installments_Period__c, &#39;Yearly&#39;)
        ),
        ISPICKVAL(RecurSoft__Revenue_Recognition_Method__c, &#39;Revenue Transaction&#39;)
    )
)</errorConditionFormula>
        <errorMessage>Revenue transaction recognition method with  &#39;Milestones&#39; billing frequency can not be selected with any installments periods.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__MilestonesWithRevRecMethod</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    AND(
        ISPICKVAL(RecurSoft__Billing_Frequency__c, &#39;Milestones&#39;),
        ISPICKVAL(RecurSoft__Revenue_Recognition_Method__c, &#39;Revenue Transaction&#39; )
    )
)</errorConditionFormula>
        <errorMessage>Revenue Transaction recognition method cannot be selected with Milestones billing frequency.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__MonthlyRevenueTransaction</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    AND(
        ISPICKVAL(RecurSoft__Billing_Frequency__c, &#39;Monthly&#39;),
        OR(
          ISPICKVAL(RecurSoft__Rev_Installments_Period__c, &#39;Milestones&#39;),
          ISPICKVAL(RecurSoft__Rev_Installments_Period__c, &#39;One-Time&#39;)
        ),
        ISPICKVAL(RecurSoft__Revenue_Recognition_Method__c, &#39;Revenue Transaction&#39;)
    )
)</errorConditionFormula>
        <errorMessage>Revenue transaction recognition method cannot be selected with &#39;Milestones&#39; or &#39;One-Time&#39; installments period and &#39;Monthly&#39; billing frequency.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__One_time_installments_for_yearly_freq</fullName>
        <active>true</active>
        <description>Validates that when Rev Installments Period=One-Time, then Billing Frequency cannot equal Yearly</description>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    AND(
         ISPICKVAL(RecurSoft__Billing_Frequency__c, &quot;Yearly&quot; ),
         ISPICKVAL(RecurSoft__Rev_Installments_Period__c, &quot;One-Time&quot;)
    )
)</errorConditionFormula>
        <errorMessage>When Rev Installments Period is One-Time, then Billing Frequency cannot be Yearly.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__Products_Price_Based_On_required</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    ISBLANK(TEXT(RecurSoft__Product_Based_On__c))
)</errorConditionFormula>
        <errorDisplayField>RecurSoft__Product_Based_On__c</errorDisplayField>
        <errorMessage>Please select a value for Product Price Based On.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__Product_Code_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    ISBLANK(ProductCode)
)</errorConditionFormula>
        <errorDisplayField>ProductCode</errorDisplayField>
        <errorMessage>Please select a value for the Product Code.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__QuarterlyRevenueTransaction</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    AND(
        ISPICKVAL(RecurSoft__Billing_Frequency__c, &#39;Quarterly&#39;),
        OR(
            ISPICKVAL(RecurSoft__Rev_Installments_Period__c, &#39;Milestones&#39;),
            ISPICKVAL(RecurSoft__Rev_Installments_Period__c, &#39;One-Time&#39;),
            ISPICKVAL(RecurSoft__Rev_Installments_Period__c, &#39;Monthly&#39;)
        ),
        ISPICKVAL(RecurSoft__Revenue_Recognition_Method__c, &#39;Revenue Transaction&#39;)
    )
)</errorConditionFormula>
        <errorMessage>Revenue transaction recognition method cannot be selected with &#39;Milestones&#39;, &#39;One-Time&#39;  or &#39;Monthly&#39; installments period and &#39;Quarterly&#39; billing frequency.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__Revenue_Schedule_Type_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    ISBLANK(TEXT(RecurSoft__Revenue_Schedule_Type__c))
)</errorConditionFormula>
        <errorDisplayField>RecurSoft__Revenue_Schedule_Type__c</errorDisplayField>
        <errorMessage>Please select a value for Revenue Schedule Type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__Rev_Installments_Period_required</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    AND(
        ISBLANK(TEXT(RecurSoft__Rev_Installments_Period__c)), 
        ISPICKVAL(RecurSoft__Revenue_Recognition_Method__c, &#39;Revenue Transaction&#39;)
    )
)</errorConditionFormula>
        <errorDisplayField>RecurSoft__Rev_Installments_Period__c</errorDisplayField>
        <errorMessage>Please select a value for Rev Installments period.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__Rev_Rec_End_Based_On_required</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    ISBLANK(TEXT(RecurSoft__Rev_Rec_End_Based_On__c))
)</errorConditionFormula>
        <errorDisplayField>RecurSoft__Rev_Rec_End_Based_On__c</errorDisplayField>
        <errorMessage>Please select a value for Rev Rec End Based On.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__Rev_Rec_Start_Off_Set</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    ISPICKVAL(RecurSoft__Revenue_Recognition_Method__c, &quot;Revenue Recognition Template&quot;) &amp;&amp; ISBLANK(RecurSoft__Rev_Rec_Start__c)
)</errorConditionFormula>
        <errorDisplayField>RecurSoft__Rev_Rec_Start__c</errorDisplayField>
        <errorMessage>Please select a value for Rev Rec Start Off-Set.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__Rev_Rec_Start_Off_Set_Days_less_then_365</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    RecurSoft__Rev_Rec_Start__c &gt;= 365
)</errorConditionFormula>
        <errorDisplayField>RecurSoft__Rev_Rec_Start__c</errorDisplayField>
        <errorMessage>Please, enter less then 365 days</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__Rev_Rec_Template_Name</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    ISPICKVAL(RecurSoft__Revenue_Recognition_Method__c, &quot;Revenue Recognition Template&quot;) &amp;&amp; ISPICKVAL(RecurSoft__Rev_Rec_Template_Name__c, &quot;&quot;)
)</errorConditionFormula>
        <errorDisplayField>RecurSoft__Rev_Rec_Template_Name__c</errorDisplayField>
        <errorMessage>Please select a value for Rev Rec Template Name.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__SemiAnnualRevenueTransaction</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    AND(
        ISPICKVAL(RecurSoft__Billing_Frequency__c, &#39;Semi-annual&#39;),
        OR(
            ISPICKVAL(RecurSoft__Rev_Installments_Period__c, &#39;Milestones&#39;),
            ISPICKVAL(RecurSoft__Rev_Installments_Period__c, &#39;One-Time&#39;),
            ISPICKVAL(RecurSoft__Rev_Installments_Period__c, &#39;Monthly&#39;),
            ISPICKVAL(RecurSoft__Rev_Installments_Period__c, &#39;Quarterly&#39;)
        ),
        ISPICKVAL(RecurSoft__Revenue_Recognition_Method__c, &#39;Revenue Transaction&#39;)
    )
)</errorConditionFormula>
        <errorMessage>Revenue transaction recognition method with  &#39;Quarterly&#39; billing frequency can be selected only with &#39;Semi-annual&#39;  or &#39;Yearly&#39; installments periods.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__Sort_By_must_be_positive</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    RecurSoft__Sort_By__c &lt;= 0
)</errorConditionFormula>
        <errorDisplayField>RecurSoft__Sort_By__c</errorDisplayField>
        <errorMessage>Sort By must be greater than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__Usage_type_is_required</fullName>
        <active>false</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    ISPICKVAL(RecurSoft__Usage_Type__c, &#39;&#39;)
)</errorConditionFormula>
        <errorDisplayField>RecurSoft__Usage_Type__c</errorDisplayField>
        <errorMessage>Usage type is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__ValidateOverageAutomation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    RecurSoft__Overage_automation__c &amp;&amp; !ISPICKVAL(RecurSoft__Usage_Type__c , &#39;Earned Revenue&#39;)
)</errorConditionFormula>
        <errorMessage>Overage automation can be selected only with Earned Revenue usage type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__ValidateRevRecMethodAndUsageType</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    AND(
        ISPICKVAL(RecurSoft__Revenue_Recognition_Method__c, &#39;Usage Based&#39;),
        NOT(
            OR(
                ISPICKVAL(RecurSoft__Usage_Type__c, &#39;Accrued Usage&#39;),
                ISPICKVAL(RecurSoft__Usage_Type__c, &#39;Earned Revenue&#39;)
            )
        )
    )
)</errorConditionFormula>
        <errorMessage>Revenue Recognition Method with Usage Based should only be assigned to a product with Usage type with Earned Revenue or Accrued Usage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__ValidateUsageTypeWithRevenueRecMethod</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    AND(
        OR(
            ISPICKVAL(RecurSoft__Usage_Type__c, &#39;Accrued Usage&#39;),
            ISPICKVAL(RecurSoft__Usage_Type__c, &#39;Earned Revenue&#39;)
        ),
        NOT(
            ISPICKVAL(RecurSoft__Revenue_Recognition_Method__c, &#39;Usage Based&#39;)
        )
    )
)</errorConditionFormula>
        <errorMessage>Usage type with Earned Revenue or Accrued Usage should be selected only with Usage Based revenue recognition method.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__Validate_based_on_contract_sign_date</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    AND(
        OR(
            ISPICKVAL(RecurSoft__Billing_Frequency__c, &#39;One-Time&#39;),
            ISPICKVAL(RecurSoft__Billing_Frequency__c, &#39;Milestones&#39;)
        ),
        !ISPICKVAL(RecurSoft__Invoicing_Date__c, &#39;Based on contract signed date&#39;),
        !ISPICKVAL(RecurSoft__Invoicing_Date__c, &#39;Based on contract start date&#39;)
    )
)</errorConditionFormula>
        <errorMessage>One-time or Milestones products without Invoicing date based on contract start date is not allowed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__Validate_frequency_with_invoicing_date</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    AND(
        OR(
            ISPICKVAL(RecurSoft__Billing_Frequency__c, &#39;Semi-annual&#39;),
            ISPICKVAL(RecurSoft__Billing_Frequency__c, &#39;Milestones&#39;)
        ),
        OR(
            ISPICKVAL(RecurSoft__Invoicing_Date__c, &#39;First day of the month&#39;),
            ISPICKVAL(RecurSoft__Invoicing_Date__c, &#39;Last day of the month&#39;)
        )
    )
)</errorConditionFormula>
        <errorMessage>Semi-annual and Milestones products with Invoicing date on First/Last Day of the month are not allowed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__Validate_usage_type_products</fullName>
        <active>false</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    AND(
        NOT(
            OR(
                ISPICKVAL(RecurSoft__Usage_Type__c, &#39;Not Applicable&#39;),
                ISPICKVAL(RecurSoft__Usage_Type__c, &#39;Usage only&#39;), ISPICKVAL(RecurSoft__Usage_Type__c, &#39;Aggregated Monthly Minimum Amount&#39;))), 
        ISPICKVAL(RecurSoft__Product_Based_On__c, &#39;Each&#39;)
    )
)</errorConditionFormula>
        <errorMessage>Products Price Based On can be selected to Each only when Usage type is Not applicable, Usage only or Aggregated Monthly Minimum Amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__Validation_rule_billing_frequency</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    IF(
        AND(
            RecurSoft__Prorated__c == true,
            !ISPICKVAL(RecurSoft__Billing_Frequency__c, &quot;Monthly&quot;)
        ),
        TRUE,
        FALSE
    )
)</errorConditionFormula>
        <errorDisplayField>RecurSoft__Prorated__c</errorDisplayField>
        <errorMessage>Prorated only works for monthly billing.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__Values_with_prorated</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    AND(
        RecurSoft__Prorated__c,
        OR(
            ISPICKVAL(RecurSoft__Invoicing_Date__c , &#39;Based on contract start date&#39;),
            ISPICKVAL( RecurSoft__Invoicing_Date__c , &#39;Based on contract signed date&#39;),
            ISPICKVAL(RecurSoft__Invoicing_Date__c, &#39;Based on activation date&#39;)
        )
    )
)</errorConditionFormula>
        <errorMessage>You cannot have Prorated checked and Invoicing Date - Based on contract start date / Based on activation date selected at the same time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__YearlyRevenueTransaction</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    AND(
        ISPICKVAL(RecurSoft__Billing_Frequency__c, &#39;Yearly&#39;),
        OR(
            ISPICKVAL(RecurSoft__Rev_Installments_Period__c, &#39;Milestones&#39;),
            ISPICKVAL(RecurSoft__Rev_Installments_Period__c, &#39;One-Time&#39;),
            ISPICKVAL(RecurSoft__Rev_Installments_Period__c, &#39;Monthly&#39;),
            ISPICKVAL(RecurSoft__Rev_Installments_Period__c, &#39;Quarterly&#39;),
            ISPICKVAL(RecurSoft__Rev_Installments_Period__c, &#39;Semi-annual&#39;)
        ),
        ISPICKVAL(RecurSoft__Revenue_Recognition_Method__c, &#39;Revenue Transaction&#39;)
    )
)</errorConditionFormula>
        <errorMessage>Revenue transaction recognition method with  &#39;Yearly&#39; billing frequency can be selected only with &#39;Yearly&#39; installments periods.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurSoft__Yearly_Products_Only</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
    $Setup.RecurSoft__Deactivate_ISB_validations__c.RecurSoft__Is_active__c, false, 
    AND(
        RecurSoft__Yearly_Special_Behavior__c  = True,
        NOT(
            ISPICKVAL(RecurSoft__Rev_Installments_Period__c, &#39;Yearly&#39;)
        )
    )
)</errorConditionFormula>
        <errorDisplayField>RecurSoft__Yearly_Special_Behavior__c</errorDisplayField>
        <errorMessage>This can only be checked for yearly products only!</errorMessage>
    </validationRules>
    <listViews>
        <fullName>AllProducts</fullName>
        <columns>PRODUCT2.NAME</columns>
        <columns>PRODUCT2.FAMILY</columns>
        <columns>PRODUCT2.CUSTOMER_PRODUCT_ID</columns>
        <columns>PRODUCT2.ACTIVE</columns>
        <columns>Default_Contract_Term__c</columns>
        <columns>Default_Co_Terminate__c</columns>
        <columns>Default_Renewal_Rule__c</columns>
        <columns>AppAdmin_API_Name__c</columns>
        <columns>AppAdminProductKey__c</columns>
        <columns>RecurSoft__Billing_Frequency__c</columns>
        <columns>Contract_Display_Name__c</columns>
        <columns>Standard_Pricebook_Entry_Id__c</columns>
        <filterScope>Everything</filterScope>
        <label>All Products</label>
    </listViews>
</CustomObject>
