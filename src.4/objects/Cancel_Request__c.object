    <validationRules>
        <fullName>Annual_Cannot_be_a_No_Sale</fullName>
        <active>true</active>
        <description>You cannot Close an Annual Cancel Request as a No Sale</description>
        <errorConditionFormula>AND(Ispickval( Cancel_Request_Type__c , &quot;Annual&quot;),
    Ispickval(Stage__c, &quot;No Sale&quot;))</errorConditionFormula>
        <errorDisplayField>Stage__c</errorDisplayField>
        <errorMessage>You cannot mark an Annual Request as a No Sale.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cancel_Month_is_required</fullName>
        <active>true</active>
        <description>When cancelling or No Sale Cancel Month is required</description>
        <errorConditionFormula>AND(OR(ISPICKVAL(Stage__c, &quot;Cancelled&quot;),ISPICKVAL(Stage__c, &quot;No Sale&quot;)), ISBLANK(Cancel_Month__c))</errorConditionFormula>
        <errorDisplayField>Cancel_Month__c</errorDisplayField>
        <errorMessage>You must add a Cancel Month to mark this as Cancelled or No Sale</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cancel_Month_is_required_when_Cancelled</fullName>
        <active>true</active>
        <description>Makes the Cancel Month required when you change the stage to Cancelled/No Sale</description>
        <errorConditionFormula>(ISPICKVAL(  Stage__c , &quot;Cancelled&quot;) || 
 ISPICKVAL( Stage__c , &quot;No Sale&quot;)) &amp;&amp;
 ISBLANK(  Cancel_Month__c )</errorConditionFormula>
        <errorDisplayField>Cancel_Month__c</errorDisplayField>
        <errorMessage>Cancel Month is required when you change the Stage to Cancelled/No Sale</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cancel_Month_must_be_in_the_future</fullName>
        <active>true</active>
        <description>The Cancel Month should be this month or the future.</description>
        <errorConditionFormula>AND(Cancel_Month__c &lt; (TODAY()- 31) , NOT(ISBLANK(Cancel_Month__c)), $Profile.Name &lt;&gt; &quot;System Administrator&quot;, $Profile.Name &lt;&gt; &quot;Finance&quot;,$Profile.Name &lt;&gt; &quot;Client Engagement Manager&quot; , 
$Profile.Name &lt;&gt; &quot;Support Exec&quot; )</errorConditionFormula>
        <errorDisplayField>Cancel_Month__c</errorDisplayField>
        <errorMessage>The Cancel Month must be in this month or the future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cancel_Month_only_valid_when_Cancelled</fullName>
        <active>true</active>
        <description>Cancel Month should only be allowed when marked as Cancelled/No Sale</description>
        <errorConditionFormula>NOT(ISPICKVAL( Stage__c , &quot;Cancelled&quot;) ) 
&amp;&amp;
NOT(ISPICKVAL( Stage__c , &quot;No Sale&quot;) ) 
&amp;&amp;
NOT( ISBLANK ( Cancel_Month__c ))</errorConditionFormula>
        <errorDisplayField>Cancel_Month__c</errorDisplayField>
        <errorMessage>You cannot add a Cancel Month until the Stage is Cancelled or No Sale</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cancel_Month_on_1st_of_the_Month</fullName>
        <active>true</active>
        <description>The Cancel Month should be on the first and only this month or the future.</description>
        <errorConditionFormula>AND(DAY(Cancel_Month__c) &lt;&gt; 1 , NOT(ISBLANK(Cancel_Month__c)))</errorConditionFormula>
        <errorDisplayField>Cancel_Month__c</errorDisplayField>
        <errorMessage>The Cancel Month must be on the first of a month</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cancel_Request_Date_cant_be_edited</fullName>
        <active>true</active>
        <description>The Cancel Request date can only be edited in the first 7 days from creating the CRO - Matthew Schetselaar</description>
        <errorConditionFormula>AND( ISCHANGED( Cancel_Request_Date__c ),  CreatedDate &lt; Now()-7 , $Profile.Name &lt;&gt; &quot;System Administrator&quot;, $Profile.Name &lt;&gt; &quot;Finance&quot;,$Profile.Name &lt;&gt; &quot;Client Engagement Manager&quot; , 
$Profile.Name &lt;&gt; &quot;Support Exec&quot;)</errorConditionFormula>
        <errorDisplayField>Cancel_Request_Date__c</errorDisplayField>
        <errorMessage>You cannot edit the Cancel Request Date after 7 days of CRO creation.  Please see your manager.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Cancel_Requests</fullName>
        <active>true</active>
        <description>Makes it so reps can&#39;t edit closed Cancel Requests - Matthew Schetselaar</description>
        <errorConditionFormula>AND(OR(PRIORVALUE(Stage__c) = &quot;Cancelled&quot;,PRIORVALUE(Stage__c) = &quot;No Sale&quot;, PRIORVALUE(Stage__c) = &quot;Save&quot;, PRIORVALUE(Stage__c) = &quot;Resume - No Change&quot;), $Profile.Name &lt;&gt; &quot;System Administrator&quot;, $Profile.Name &lt;&gt; &quot;Finance&quot;,$Profile.Name &lt;&gt; &quot;Client Engagement Manager&quot; , 
$Profile.Name &lt;&gt; &quot;Support Exec&quot;)</errorConditionFormula>
        <errorMessage>You cannot edit a closed Cancel Request.  Please see your manager.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Current_Contract_Info_Required</fullName>
        <active>true</active>
        <description>Requires Original Setup Fee, Current MSF, Competitor, Most Recent Contract Date and Cancellation Reasons before closing.</description>
        <errorConditionFormula>AND( 
    OR(ISPICKVAL( Stage__c, &quot;Save&quot;),
       ISPICKVAL( Stage__c, &quot;No Sale&quot;),
       ISPICKVAL( Stage__c, &quot;Cancelled&quot;),
       ISPICKVAL( Stage__c, &quot;Resume No-Change&quot;)
       ),
     OR ( ISBLANK( Original_Setup_Fee__c ), 
          ISBLANK( Current_MSF__c ),
          ISBLANK( Most_Recent_Contract_Date__c ), 
          ISPICKVAL( Cancellation_Reasons__c , &quot;&quot;),
          ISPICKVAL(  Cancel_Request_Type__c , &quot;&quot;)
         )
   )</errorConditionFormula>
        <errorMessage>The following fields are required when closing this Request: Original Setup Fee, Current MSF, Most Recent Contract Date and Cancellation Reasons.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Current_Feature_only_when_selected</fullName>
        <active>true</active>
        <description>Requires a Current Feature if that is a Cancel Reason</description>
        <errorConditionFormula>Not(CONTAINS(Text(Cancellation_Reasons__c), &quot;Functionality of Current Feature&quot;)) &amp;&amp;  Not(ISBLANK( Current_Feature__c  ))</errorConditionFormula>
        <errorDisplayField>Current_Feature__c</errorDisplayField>
        <errorMessage>You cannot select a Current feature without making Functionality of Current Feature the Cancellation Reason</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Current_Feature_Required</fullName>
        <active>true</active>
        <description>Requires a Current Feature if that is a Cancel Reason</description>
        <errorConditionFormula>CONTAINS(Text(Cancellation_Reasons__c), &quot;Functionality of Current Feature&quot;) &amp;&amp;  ISBLANK( Current_Feature__c  )</errorConditionFormula>
        <errorDisplayField>Current_Feature__c</errorDisplayField>
        <errorMessage>You must select at least one Current Feature</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ET_Reason_Required_for_BK_Collections</fullName>
        <active>true</active>
        <description>If BK/Collections is selected then the ET Reasons is required</description>
        <errorConditionFormula>ISPICKVAL (  Cancellation_Reasons__c  , &quot;Bankruptcy/Collections/Out of business&quot; ) &amp;&amp;
ISPICKVAL (  E_T_Reason__c , &quot;&quot; )</errorConditionFormula>
        <errorDisplayField>E_T_Reason__c</errorDisplayField>
        <errorMessage>E.T. Reason is required when Bankruptcy/Collections/Out of business is selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ET_reason_required_for_early_cancels</fullName>
        <active>true</active>
        <description>If the Cancel Month is before the Projected Renewal date on Annuals SR and HG CRO&#39;s the ET Reason is required</description>
        <errorConditionFormula>( RecordType.Name = &quot;SR Platform&quot; ||
  RecordType.Name = &quot;Healthgrades&quot; ) &amp;&amp;

 ISPICKVAL (Cancel_Request_Type__c , &quot;Annual&quot; ) &amp;&amp;
 Projected_Contract_Expiration_Renewal__c &gt; Cancel_Month__c &amp;&amp;
 ISPICKVAL (  E_T_Reason__c  , &quot;&quot; )</errorConditionFormula>
        <errorDisplayField>E_T_Reason__c</errorDisplayField>
        <errorMessage>E.T. Reason is Required when the Cancel Month is before the Projected renewal date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Final_Cancellation_Details_required</fullName>
        <active>false</active>
        <description>Requires something in the final cancellation details field when closed as a Cancelled or No Sale</description>
        <errorConditionFormula>(ISPICKVAL ( Stage__c , &quot;Cancelled&quot; ) ||
 ISPICKVAL ( Stage__c , &quot;No Sale&quot; )) &amp;&amp;
 ISBLANK ( Cancellation_Details__c )</errorConditionFormula>
        <errorDisplayField>Cancellation_Details__c</errorDisplayField>
        <errorMessage>Please enter something in the  Final Cancellation Details field when closing as Cancelled/No Sale</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_Competitor</fullName>
        <active>true</active>
        <description>Requires a competitor is &quot;Other Competitor&quot; is selected</description>
        <errorConditionFormula>Includes( Competitor__c, &quot;Other(Note name in Other Competitor)&quot;) &amp;&amp;  Other_Competittor__c =null</errorConditionFormula>
        <errorDisplayField>Other_Competittor__c</errorDisplayField>
        <errorMessage>Please add the Other Competitor name.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_Competitor_2</fullName>
        <active>true</active>
        <description>Doesn&#39;t allow Other Competitor in free text field without selecting it in the picklist</description>
        <errorConditionFormula>not(includes(Competitor__c,&quot;Other(Note name in Other Competitor)&quot;)) &amp;&amp; 
 Other_Competittor__c  != null</errorConditionFormula>
        <errorDisplayField>Competitor__c</errorDisplayField>
        <errorMessage>Please add &quot;Other(Note name in Other Competitor)&quot; as a Competitor</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Cancel_Request_Type</fullName>
        <active>true</active>
        <description>Makes it so Cancel Request Type is required after creation</description>
        <errorConditionFormula>AND(CreatedDate  &lt;&gt; Now(), ISPICKVAL( Cancel_Request_Type__c , &quot;&quot;))</errorConditionFormula>
        <errorDisplayField>Cancel_Request_Type__c</errorDisplayField>
        <errorMessage>Please select Annual or Trial for Cancel Request Type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Initial_Description_on_closing</fullName>
        <active>true</active>
        <description>When a CRO is closed the Initial Description should be filled out.</description>
        <errorConditionFormula>AND (OR (ISPICKVAL(Stage__c , &quot;Cancelled&quot;          ),
         ISPICKVAL(Stage__c , &quot;No Sale&quot;            ), 
         ISPICKVAL(Stage__c , &quot;Save&quot;               ),
         ISPICKVAL(Stage__c , &quot;Resume - No Change&quot; )
         ) ,
     ISBLANK ( Description__c )
     )</errorConditionFormula>
        <errorDisplayField>Description__c</errorDisplayField>
        <errorMessage>You must provide Initial Details before closing a Cancel Request</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Months_paid_remaining_on_Saves</fullName>
        <active>true</active>
        <description>For all Saves Months paid for trials and months remaining for annuals is required</description>
        <errorConditionFormula>ISPICKVAL ( Stage__c , &quot;Save&quot; ) &amp;&amp;

((ISPICKVAL (Cancel_Request_Type__c , &quot;Annual&quot; ) &amp;&amp;  ISNULL (Months_Remaining_Annual__c ) ) || (ISPICKVAL (Cancel_Request_Type__c , &quot;Trial&quot; ) &amp;&amp;  ISNULL (Months_Paid_Trial__c )))</errorConditionFormula>
        <errorMessage>When closing as a Save, Months Paid is required for Trials and Months Remaining is required for Annuals.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Projected_Renewal_when_closing</fullName>
        <active>true</active>
        <description>When a CRO is closed, the Projected Renewal Date is needed.</description>
        <errorConditionFormula>AND (OR (ISPICKVAL(Stage__c , &quot;Cancelled&quot;          ),
         ISPICKVAL(Stage__c , &quot;No Sale&quot;            ), 
         ISPICKVAL(Stage__c , &quot;Save&quot;               ),
         ISPICKVAL(Stage__c , &quot;Resume - No Change&quot; )
         ) ,
     ISBLANK ( Projected_Contract_Expiration_Renewal__c )
     )</errorConditionFormula>
        <errorDisplayField>Projected_Contract_Expiration_Renewal__c</errorDisplayField>
        <errorMessage>The Projected Contract Expiration/Renewal Date is required when closing a Cancel Request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Save_Info</fullName>
        <active>true</active>
        <description>Requires certain fields when closing as a Save.</description>
        <errorConditionFormula>AND(ISPICKVAL(Stage__c, &quot;Save&quot;),
    OR(ISBLANK(Save_MSF__c),
       ISBLANK(Months_Gained__c)
    )
)</errorConditionFormula>
        <errorMessage>You must put in Save MSF and Months Gained when closing as a Save.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Specific_Cancel_Reasons</fullName>
        <active>true</active>
        <description>If a Cancel Reason is because of a feature, require the rep to enter in a specific reason</description>
        <errorConditionFormula>AND( OR ( ISPICKVAL( Cancellation_Reasons__c , &quot;Not satisfied - current features&quot;) , 
          ISPICKVAL( Cancellation_Reasons__c , &quot;Not satisfied - unavailable features&quot; ) ) , ISBLANK(Specific_reasons_for_unavailable_current__c) , OR(ISPICKVAL( Stage__c, &quot;Save&quot;), 
ISPICKVAL( Stage__c, &quot;No Sale&quot;), 
ISPICKVAL( Stage__c, &quot;Cancelled&quot;), 
ISPICKVAL( Stage__c, &quot;Resume No-Change&quot;) 
))</errorConditionFormula>
        <errorDisplayField>Specific_reasons_for_unavailable_current__c</errorDisplayField>
        <errorMessage>You must provide a specific reason before closing.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Unavailable_Feature_Only_when_Other</fullName>
        <active>true</active>
        <description>Only allow text in New Unavailable Feature when Other is selected</description>
        <errorConditionFormula>Not(Includes(Unavailable_Feature__c , &quot;Other&quot;)) &amp;&amp;  Not(ISBLANK (New_Unavailable_Feature__c ))</errorConditionFormula>
        <errorDisplayField>New_Unavailable_Feature__c</errorDisplayField>
        <errorMessage>You can only enter in a new Unavailable Feature when &quot;Other&quot; is selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Unavailable_Feature_only_when_selected</fullName>
        <active>true</active>
        <description>Makes it so Unavailable features can only be selected when that cancel reason is selected</description>
        <errorConditionFormula>NOT(CONTAINS(Text(Cancellation_Reasons__c), &quot;Unavailable Feature&quot;)) &amp;&amp;  NOT(ISBLANK(Unavailable_Feature__c ))</errorConditionFormula>
        <errorDisplayField>Unavailable_Feature__c</errorDisplayField>
        <errorMessage>You Cannot select an Unavailable Feature unless you&#39;ve selected that option from the Cancel Reasons</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Unavailable_Feature_Other_requires_text</fullName>
        <active>true</active>
        <description>Require New Unavailable Features when &quot;Other&quot; is selected.</description>
        <errorConditionFormula>Includes(Unavailable_Feature__c , &quot;Other&quot;) &amp;&amp;  ISBLANK (New_Unavailable_Feature__c )</errorConditionFormula>
        <errorDisplayField>New_Unavailable_Feature__c</errorDisplayField>
        <errorMessage>You must enter a New Unavailable Feature when &quot;Other&quot; is selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Unavailable_Feature_Required</fullName>
        <active>true</active>
        <description>Requires an Unavailable Feature if that is a Cancel Reason</description>
        <errorConditionFormula>CONTAINS(Text(Cancellation_Reasons__c), &quot;Unavailable Feature&quot;) &amp;&amp;  ISBLANK(Unavailable_Feature__c )</errorConditionFormula>
        <errorDisplayField>Unavailable_Feature__c</errorDisplayField>
        <errorMessage>You must select at least one Unavailable Feature</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Waived_Months_Accepted</fullName>
        <active>true</active>
        <description>Requires a number when marking Wavied Months Offered as Accepted</description>
        <errorConditionFormula>AND (ISPICKVAL( Save_Offer_Waived_Months__c , &quot;Accepted&quot;) , ISNULL (Number_of_waived_months_offered__c ))</errorConditionFormula>
        <errorDisplayField>Number_of_waived_months_offered__c</errorDisplayField>
        <errorMessage>You must provide the number of free months if the offer was accepted</errorMessage>
    </validationRules>
