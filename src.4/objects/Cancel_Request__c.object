        <fullName>Final_Cancel_Status__c</fullName>
        <externalId>false</externalId>
        <formula>if(and(text(Cancel_Request_Type__c)=&quot;Annual&quot;,text(Stage__c)=&quot;Cancelled&quot;),&quot;Annual Cancel&quot;, 
if(and(text(Cancel_Request_Type__c)=&quot;Trial&quot;,text(Stage__c)= &quot;Cancelled&quot;), &quot;Trial Cancel&quot;, 
if(and(text(Cancel_Request_Type__c)=&quot;Trial&quot;,text(Stage__c)= &quot;No Sale&quot;),&quot;No Sale&quot;, 
&quot;&quot;)))</formula>
        <label>Final Cancel Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Keep_Amount__c</fullName>
        <externalId>false</externalId>
        <formula>IF ( ISPICKVAL( Cancel_Request_Type__c , &quot;Trial&quot;), ( Months_Paid_Trial__c * Current_MSF__c ) + (( Save_Term__c - New_Free_Months__c ) * Save_MSF__c ) , (( Save_Term__c - New_Free_Months__c ) * Save_MSF__c ) - ( Months_Remaining_Annual__c * Current_MSF__c))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Keep Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Keep_Percentage__c</fullName>
        <externalId>false</externalId>
        <formula>IF(  ISBLANK(Current_MSF__c), 0, 
     IF ( ISPICKVAL( Cancel_Request_Type__c , &quot;Trial&quot;), Keep_Amount__c / (24 * Current_MSF__c ) , 
     If (Months_Remaining_Annual__c = 0 , (( Save_Term__c - New_Free_Months__c ) * Save_MSF__c ) / ( 12 * Current_MSF__c ), 
     (( Save_Term__c - New_Free_Months__c ) * Save_MSF__c ) / ( Months_Remaining_Annual__c * Current_MSF__c))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Keep Percentage</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
