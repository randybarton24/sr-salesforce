            <script>
                
                j$ = jQuery.noConflict();
            	j$(document).ready(function() {
                
                if(x$("input[id*='formValidated']").is(':checked')) {                    
                    processing();                    
                } else {                    
                    notProcessing();
                }
            });
            
            function processing() {
                j$('.waitingGifDiv').show();
                j$('.SubmitButtonDiv').hide();
            }
            
            function notProcessing() {
                j$('.waitingGifDiv').hide();
                j$('.SubmitButtonDiv').show();
            }
            
            </script>
            
            <script type="text/javascript">
            
            var x$ = jQuery.noConflict();
            
            function validateFormEmail(elementRef) {                
                if (elementRef.value == '') return true;
                var x = elementRef.value;
                var atpos = x.indexOf("@");
                var dotpos = x.lastIndexOf(".");
                var whitespace = x.indexOf(' ');
                
                if ((atpos < 1 || dotpos < atpos + 2 || dotpos + 2 >= x.length || whitespace >= 0)) {                    
                    alert("Not a valid e-mail address " + x);
                    elementRef.value = '';
                    return false;
                }
            }
            
            //Alow integer only
            function AllowNumericsOnly(elementRef, event) {                
                var keyCodeEntered = (event.which) ? event.which : (window.event.keyCode) ? window.event.keyCode : -1;                                
                
                if(((keyCodeEntered >= 48) && (keyCodeEntered <= 57)) || keyCodeEntered === 8 || keyCodeEntered === 46 || keyCodeEntered === 9 || keyCodeEntered === 37 || keyCodeEntered === 39 || keyCodeEntered === 16 || keyCodeEntered === 189 || keyCodeEntered === 109 || ((keyCodeEntered >= 96) && (keyCodeEntered <= 105))) return true;
                else return false;
            }
            
            function checkRequired(elementRef) {                
                if (elementRef.value.length == 0)  elementRef.style.background = 'Pink';
                else elementRef.style.background = 'White';                
            }
            
            function CheckTerms() {                
                var formError = false;                
                if(document.getElementById('page:contractform:hasSR').checked || document.getElementById('page:contractform:hasSRMonthly').checked || document.getElementById('page:contractform:hasSRAdvance').checked || document.getElementById('page:contractform:hasSRAnnual').checked) {                    
                    if(document.getElementById('page:contractform:businessname').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:businessname'));
                        formError = true;
                    }                    
                    if(document.getElementById('page:contractform:street').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:street'));
                        formError = true;
                    }                    
                    if(document.getElementById('page:contractform:city').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:city'));
                        formError = true;
                    }                    
                    if(document.getElementById('page:contractform:zip').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:zip'));
                        formError = true;
                    }                    
                    if(document.getElementById('page:contractform:contact2FirstName').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:contact2FirstName'));
                        formError = true;
                    }                    
                    if(document.getElementById('page:contractform:contact2LastName').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:contact2LastName'));
                        formError = true;
                    }                    
                    if(document.getElementById('page:contractform:contact2Phone').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:contact2Phone'));
                        formError = true;
                    }                    
                    if(document.getElementById('page:contractform:contact2email').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:contact2email'));
                    }                    
                    if(document.getElementById('page:contractform:contact1FirstName').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:contact1FirstName'));
                        formError = true;
                    }                    
                    if(document.getElementById('page:contractform:contact1LastName').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:contact1LastName'));
                        formError = true;
                    }                    
                    if(document.getElementById('page:contractform:contact1Phone').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:contact1Phone'));
                        formError = true;
                    }                    
                    if(document.getElementById('page:contractform:contact1email').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:contact1email'));
                        formError = true;
                    }                    
                    if(document.getElementById('page:contractform:contact4FirstName').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:contact4FirstName'));
                        formError = true;
                    }                    
                    if(document.getElementById('page:contractform:contact4LastName').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:contact4LastName'));
                        formError = true;
                    }                    
                    if(document.getElementById('page:contractform:contact4Phone').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:contact4Phone'));
                        formError = true;
                    }                    
                    if(document.getElementById('page:contractform:contact4email').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:contact4email'));
                        formError = true;
                    }
                    
                    if(document.getElementById('page:contractform:contact3FirstName').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:contact3FirstName'));
                        formError = true;
                    }
                    
                    if(document.getElementById('page:contractform:contact3LastName').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:contact3LastName'));
                        formError = true;
                    }
                    
                    if(document.getElementById('page:contractform:contact3Phone').value.length == 0) {
