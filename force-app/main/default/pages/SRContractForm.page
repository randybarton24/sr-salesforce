            <script type="text/javascript">

            function isValidPhone(idPhone) {                
                var phoneNumber = document.getElementById("page:contractform:"+idPhone).value;                              
                var digits = phoneNumber.replace(/-/g, "");
                if (digits != "") document.getElementById("page:contractform:"+idPhone).value = digits.substring(0, 3) + "-" + digits.substring(3, 6) + "-" + digits.substring(6, 10);

            }
            
            // Used to determine if Contract Signer should be copied from Primary or Business Contact or Billing Contract
            function setContractSigner(option) {
                var CSPrimary = false;
                var CSBC = false;
                var CSBilling = false;
                
                if (option === "primary"){
                	document.getElementById('page:contractform:CSBC').checked = false;
                	document.getElementById('page:contractform:CSBilling').checked = false;
                	CSPrimary = document.getElementById('page:contractform:CSPrimary').checked;
                }
                else if (option === "bc"){
                	document.getElementById('page:contractform:CSPrimary').checked = false;
                	document.getElementById('page:contractform:CSBilling').checked = false;
                    CSBC = document.getElementById('page:contractform:CSBC').checked;
                }
                else if (option === "billing"){
                	document.getElementById('page:contractform:CSPrimary').checked = false;
                	document.getElementById('page:contractform:CSBC').checked = false;
                    CSBilling = document.getElementById('page:contractform:CSBilling').checked;
                }
                
                //contact1 is the Primary (CSPrimary)
                var contact1FirstName = document.getElementById('page:contractform:contact1FirstName').value;
                var contact1LastName = document.getElementById('page:contractform:contact1LastName').value;
                var contact1Title = document.getElementById('page:contractform:contact1Title').value;
                var contact1Phone = document.getElementById('page:contractform:contact1Phone').value;
                var contact1email = document.getElementById('page:contractform:contact1email').value;
                
                //contact2 is the Business Contact (CSBC) 
                var contact2FirstName = document.getElementById('page:contractform:contact2FirstName').value;
                var contact2LastName = document.getElementById('page:contractform:contact2LastName').value;
                var contact2Title = document.getElementById('page:contractform:contact2Title').value;
                var contact2Phone = document.getElementById('page:contractform:contact2Phone').value;
                var contact2email = document.getElementById('page:contractform:contact2email').value;
                
                //contract4 is the Billing Contact
                var contact4FirstName = document.getElementById('page:contractform:contact4FirstName').value;
                var contact4LastName = document.getElementById('page:contractform:contact4LastName').value;
                var contact4Title = document.getElementById('page:contractform:contact4Title').value;
                var contact4Phone = document.getElementById('page:contractform:contact4Phone').value;
                var contact4email = document.getElementById('page:contractform:contact4email').value;
                                
                document.getElementById('page:contractform:contact3FirstName').value = CSPrimary ? contact1FirstName : (CSBC ? contact2FirstName : (CSBilling ? contact4FirstName : null));
                document.getElementById('page:contractform:contact3LastName').value = CSPrimary ? contact1LastName : (CSBC ? contact2LastName : (CSBilling ? contact4LastName : null));
                document.getElementById('page:contractform:contact3Title').value = CSPrimary ? contact1Title : (CSBC ? contact2Title : (CSBilling ? contact4Title : null));
                document.getElementById('page:contractform:contact3Phone').value = CSPrimary ? contact1Phone : (CSBC ? contact2Phone : (CSBilling ? contact4Phone : null));
                document.getElementById('page:contractform:contact3email').value = CSPrimary ? contact1email : (CSBC ? contact2email : (CSBilling ? contact4email : null));
                
            }
            
            //Used to set the Billing contract as either Primary or as the Business Contact
            function setBilling(option){               
                var BCPrimary=false;
                var BCBC = false;
                
                if (option === "primary") {
                	document.getElementById('page:contractform:BCBC').checked = false;
                    BCPrimary = document.getElementById('page:contractform:BCPrimary').checked
                }
                if (option === "bc") {
                    document.getElementById('page:contractform:BCPrimary').checked = false;                
