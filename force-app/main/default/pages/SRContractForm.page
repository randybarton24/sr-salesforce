                    }
                    
                    if(document.getElementById('page:contractform:contact3email').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:contact3email'));
                        formError = true;
                    }
                    
                    if(document.getElementById('page:contractform:billingstreet').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:billingstreet'));
                        formError = true;
                    }
                    
                    if(document.getElementById('page:contractform:billingcity').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:billingcity'));
                        formError = true;
                    }
                    
                    if(document.getElementById('page:contractform:billingzip').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:billingzip'));
                        formError = true;
                    }
                    
                    if(document.getElementById('page:contractform:contract_cardType').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:contract_cardType'));
                        formError = true;
                    }
                    
                    if(document.getElementById('page:contractform:contract_cardNumber').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:contract_cardNumber'));
                        formError = true;
                    }
                    
                    if(document.getElementById('page:contractform:cardMonth').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:cardMonth'));
                        formError = true;
                    }
                    
                    if(document.getElementById('page:contractform:cardYear').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:cardYear'));
                        formError = true;
                    }
                    
                    if(document.getElementById('page:contractform:cardName').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:cardName'));
                        formError = true;
                    }
                    
                    if(document.getElementById('page:contractform:contractSigner').value.length == 0) {
                        checkRequired(document.getElementById('page:contractform:contractSigner'));
                        formError = true;
                    }
                }
                
                if(formError == false) {                    
                    validateForm();
                    processing();                    
                    setTimeout(doSubmit, 2000);                    
                } 
                else {                    
                    notProcessing();
                    alert("Please populate all required fields.");
                }
            }
            
            function doSubmit() {
                
                if(x$("input[id*='formValidated']").is(':checked')) {                    
                    document.getElementById('page:contractform:commandButton').click();
                    processing();                    
