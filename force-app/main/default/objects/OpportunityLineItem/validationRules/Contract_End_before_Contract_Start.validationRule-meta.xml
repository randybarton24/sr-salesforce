<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Contract_End_before_Contract_Start</fullName>
    <active>true</active>
    <errorConditionFormula>AND( NOT( ISNULL( RecurSoft__Contract_Term_End_Date_Editable__c ) ) , RecurSoft__Contract_Term_End_Date_Editable__c &lt;= RecurSoft__Contract_Term_Start_Date_New__c )</errorConditionFormula>
    <errorDisplayField>RecurSoft__Contract_Term_End_Date_Editable__c</errorDisplayField>
    <errorMessage>Contract Term End Date must be greater than Contract Term Start Date</errorMessage>
</ValidationRule>
