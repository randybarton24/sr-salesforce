<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Non_Medical_Conversion</fullName>
    <active>false</active>
    <errorConditionFormula>AND( 
IsConverted = true, 
ISPICKVAL(Industry, &quot;Non-Medical&quot;)
)</errorConditionFormula>
    <errorMessage>You cannot convert a Non-Medical Account</errorMessage>
</ValidationRule>
