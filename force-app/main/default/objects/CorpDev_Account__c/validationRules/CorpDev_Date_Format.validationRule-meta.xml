<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CorpDev_Date_Format</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX(Date_of_Latest_Investment__c ,&quot;^((0[1-9])|(1[0-2]))/[0-9]{4}$&quot;)) &amp;&amp;
NOT(ISBLANK( Date_of_Latest_Investment__c ))</errorConditionFormula>
    <errorDisplayField>Date_of_Latest_Investment__c</errorDisplayField>
    <errorMessage>Please use format MM/YYYY</errorMessage>
</ValidationRule>
