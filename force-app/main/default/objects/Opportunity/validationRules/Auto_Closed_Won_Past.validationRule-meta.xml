<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Auto_Closed_Won_Past</fullName>
    <active>true</active>
    <errorConditionFormula>ISCHANGED( Auto_Closed_Won_Date__c ) &amp;&amp;
Auto_Closed_Won_Date__c &lt; TODAY()</errorConditionFormula>
    <errorDisplayField>Auto_Closed_Won_Date__c</errorDisplayField>
    <errorMessage>The Auto Closed Won Date cannot be in the past</errorMessage>
</ValidationRule>
