<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Reactivation_not_allowed</fullName>
    <active>true</active>
    <errorConditionFormula>AND( NOT(ISBLANK( Date_Deactivated__c )),  Active__c = true)</errorConditionFormula>
    <errorMessage>You cannot activate a Playbook that was previously active</errorMessage>
</ValidationRule>
