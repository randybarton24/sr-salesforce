<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Eligible_for_a_Survey__c</fullName>
    <description>Checks to see if the contact is eligible to receive a Post-Case Survey</description>
    <externalId>false</externalId>
    <formula>Or(  ISBLANK(Last_Survey_Sent_Date__c), Last_Survey_Sent_Date__c &lt; Today()-15)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Eligible for a Survey</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
